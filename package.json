    "release": "np --no-tests"
  },
  "np": {
    "yarn": false,
    "contents": "dist"
  },
  "devDependencies": {
    "boxen": "5.0.1",
    "chalk": "4.1.1",
    "np": "7.5.0"
  },
  "repository": {
