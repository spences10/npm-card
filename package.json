    "url": "git+https://github.com/spences10/spences10.git"
  },
  "keywords": [
    "npm",
    "card",
    "business card",
    "user details"
  ],
  "author": "Scott Spence <spences10apps@gmail.com> (https://scottspence.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spences10/spences10/issues"
  },
  "homepage": "https://github.com/spences10/spences10#readme",
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "printWidth": 70,
    "arrowParens": "avoid",
    "proseWrap": "always"
  }
